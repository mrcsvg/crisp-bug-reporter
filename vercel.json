{
  "version": 2,
  "builds": [
    {
      "src": "plugin/dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/plugin/dist/$1"
    }
  ]
}
